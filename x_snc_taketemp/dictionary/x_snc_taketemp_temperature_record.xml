<?xml version="1.0" encoding="UTF-8"?><database>
    <element label="Temperature Record" max_length="40" name="x_snc_taketemp_temperature_record" sizeclass="15" type="collection">
        <element label="Date" max_length="40" name="date" type="glide_date_time"/>
        <element label="Notes" max_length="1000" name="notes" type="string"/>
        <element choice="1" label="Temperature" max_length="40" name="temperature" type="choice">
            <choice>
                <element inactive_on_update="false" label="Minore di 37" sequence="5" value="&lt;37"/>
                <element inactive_on_update="false" label="Tra 37 e 38" sequence="10" value="37&lt;38"/>
                <element inactive_on_update="false" label="Maggiore di 38" sequence="15" value="&gt;38"/>
            </choice>
        </element>
        <element label="User" max_length="32" name="user" reference="sys_user" type="reference"/>
        <index name="index">
            <element name="user"/>
        </index>
    </element>
</database>
